{
  "roles": [
    "admin",
    "user"
  ],
  "strategy": "JWT",
  "permissions": {
    "user": [
      "profile:read",
      "profile:write"
    ],
    "admin": [
      "user:read",
      "user:write",
      "user:delete",
      "system:admin"
    ]
  },
  "tokenExpiry": "15m",
  "jwtAlgorithm": "HS256",
  "rateLimiting": {
    "api": {
      "window": "1h",
      "requests": 1000
    },
    "login": {
      "window": "15m",
      "requests": 5
    }
  },
  "hashAlgorithm": "bcrypt",
  "refreshTokenExpiry": "7d"
}